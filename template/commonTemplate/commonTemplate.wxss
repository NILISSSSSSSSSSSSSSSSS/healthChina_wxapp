

.onReachBottomView{
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 80rpx;
  line-height: 80rpx;
  color: #999999;
  font-size: 25rpx;
}
.onReachBottom{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 0;
  background-color: #ffffff;
  margin: 0 10rpx 0 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
  color: #999999;
}
.onReachBottom::after{
  border-radius: 0;
  border: none;
}
